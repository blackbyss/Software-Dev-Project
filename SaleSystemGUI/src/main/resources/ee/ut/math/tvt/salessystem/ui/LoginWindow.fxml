<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="loginButton" layoutX="200.0" layoutY="206.0" mnemonicParsing="false" onMouseClicked="#loginButtonClicked" prefHeight="25.0" prefWidth="100.0" text="Login" />
      <TextField fx:id="inputUser" layoutX="176.0" layoutY="119.0" prefHeight="30.0" promptText="Username" />
      <PasswordField fx:id="inputPassword" layoutX="176.0" layoutY="167.0" prefHeight="30.0" promptText="Password" />
      <Label fx:id="loginStatus" layoutX="64.0" layoutY="60.0" prefHeight="30.0" prefWidth="272.0" text="Status" textFill="RED">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <HBox layoutY="265.0" prefHeight="25.0" prefWidth="500.0">
         <children>
            <Button fx:id="selectCashier" mnemonicParsing="false" onMouseClicked="#cashierButtonClicked" prefWidth="125.0" text="Cashier" />
            <Button fx:id="selectWarehouse" mnemonicParsing="false" onMouseClicked="#warehouseButtonClicked" prefWidth="125.0" text="Warehouse" />
            <Button fx:id="selectManager" mnemonicParsing="false" onMouseClicked="#managerClicked" prefWidth="125.0" text="Manager" />
            <Button fx:id="selectClient" mnemonicParsing="false" onMouseClicked="#clientClicked" prefWidth="125.0" text="Client" />
         </children>
      </HBox>
   </children>
</AnchorPane>
